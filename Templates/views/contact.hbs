   <h3>Email</h3>
   <p>abcd@gmail.com</p>
</div>
</div>
       </div>
 
   <div class="contactform">
       <form action="/contact" method="POST">
           <h2>Send Message</h2>
           <div class="inputbox">
               <input type="text" name="name" id="exampleInputName" required="required">
               <span>Full Name</span>
           </div>
           <div class="inputbox">
               <input type="email" name="email" aria-describedby="emailHelp" id="exampleInputEmail" required="required">
               <span>Email</span>
           </div>
           <div class="inputbox">
              <textarea required="required" name="message" id="exampleInputMessage"></textarea>
               <span>Type your message....</span>
           </div>
           <div class="inputbox">
             <button type="submit">SEND</button>
           </div>
       </form>
   </div>
</div>
</div>
</section>
{{>footer}}
  

</body>
</html>